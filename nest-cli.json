{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/projet-hecate/src",
  "monorepo": true,
  "root": "apps/projet-hecate",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/projet-hecate/tsconfig.app.json"
  },
  "projects": {
    "projet-hecate": {
      "type": "application",
      "root": "apps/projet-hecate",
      "entryFile": "main",
      "sourceRoot": "apps/projet-hecate/src",
      "compilerOptions": {
        "tsConfigPath": "apps/projet-hecate/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "user-library": {
      "type": "application",
      "root": "apps/user-library",
      "entryFile": "main",
      "sourceRoot": "apps/user-library/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-library/tsconfig.app.json"
      }
    },
    "hecate-dto": {
      "type": "library",
      "root": "libs/hecate-dto",
      "entryFile": "index",
      "sourceRoot": "libs/hecate-dto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/hecate-dto/tsconfig.lib.json"
      }
    },
    "hecate-compositor": {
      "type": "application",
      "root": "apps/hecate-compositor",
      "entryFile": "main",
      "sourceRoot": "apps/hecate-compositor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/hecate-compositor/tsconfig.app.json"
      }
    }
  }
}